/**
      Cell Name:        HGF011Q7E6_50V_FIL01V1
      Type:         
**/
cell (HGF011Q7E6_50V_FIL01V1) {
 area : 0 ;
 


   pin (V50A) {
           direction : inout;
           capacitance : 0.1;
   }

   pin (G50A) {
           direction : inout;
           capacitance : 0.1;
   }


   pin(OUT_50V) {
        direction : output ;
        max_capacitance : 5 ;
        min_capacitance : 0.000000 ;

   }


 bus(INSEL_50V) {
        bus_type       : "bus2";


        pin (INSEL_50V[1]) {
        direction      : input;
        max_transition : 3.000000 ;
        capacitance    : 0.1;
        }
        pin (INSEL_50V[0]) {
        direction      : input;
        max_transition : 3.000000 ;
        capacitance    : 0.1;
        }

} /* end of bus INSEL_50V */


   bus(FILS_50V) {
        bus_type       : "bus3";

       
        pin (FILS_50V[2]) {
        direction      : input;
        max_transition : 3.000000 ;
        capacitance    : 0.1;
        }
        pin (FILS_50V[1]) {
        direction      : input;
        max_transition : 3.000000 ;
        capacitance    : 0.1;
        }
        pin (FILS_50V[0]) {
        direction      : input;
        max_transition : 3.000000 ;
        capacitance    : 0.1;
        }

} /* end of bus FILS_50V */


   pin(IN0_50V) {
        direction : input ;
        clock : true ;
        max_transition : 1.000000 ;
        capacitance : 0.5 ;
   }


   pin(IN1_50V) {
        direction : input ;
        clock : true ;
        max_transition : 1.000000 ;
        capacitance : 0.5 ;
   }

   pin(IN2_50V) {
        direction : input ;
        clock : true ;
        max_transition : 1.000000 ;
        capacitance : 0.5 ;
   }
   pin(IN3_50V) {
        direction : input ;
        clock : true ;
        max_transition : 1.000000 ;
        capacitance : 0.5 ;
   }




}
/* end HGF011Q7E6_50V_FIL01V1 */


